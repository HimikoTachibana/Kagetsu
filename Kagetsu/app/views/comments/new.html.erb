<% if current_user %>
	<div class="module-header">
		<div class="module-title"><h1>You are now in World Channel!</h1></div>
	</div>
	<%= render 'form' %>
	<hr />
	<div class="wrapper">
		<div class="panel panel-default" id="comments">
			<div class="panel-body">
				<ul class="media-list">
				<%= render @comments %>
				</ul>
			</div>
		</div>
	</div>
<% else %>
	<%= render 'welcome' %>
<% end %>
